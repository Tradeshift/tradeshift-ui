{
  "name": "tradeshift-ui",
  "version": "7.2.0",
  "description": "The Tradeshift UI Library & Framework",
  "homepage": "https://ui.tradeshift.com/",
  "bugs": {
    "url": "https://github.com/Tradeshift/tradeshift-ui/issues",
    "email": "dsp@tradeshift.com"
  },
  "author": "Tradeshift UI Team",
  "contributors": [
    "Wired Earp <jmo@tradeshift.com> (https://github.com/wiredearp)",
    "Leo Zhang <lza@tradeshift.com> (https://github.com/zdlm)",
    "Daniel Spitzer <dsp@tradeshift.com> (https://github.com/sampi)"
  ],
  "license": "SEE LICENSE IN LICENSE.md",
  "engines": {
    "node": "6.10.0"
  },
  "scripts": {
    "postinstall": "cd docs && npm install",
    "lint": "npm run prettier && npm run eslint",
    "eslint": "eslint --config ./.eslintrc.json .",
    "prettier": "./node_modules/.bin/prettier --write --use-tabs --single-quote --print-width 100 \"{src,docs/src/{js/{dox-api@tradeshift.com,dox-gui@tradeshift.com},xhtml}}/**/*.js\"",
    "build": "./node_modules/.bin/grunt dist",
    "test": "npm run lint && ./node_modules/.bin/grunt jasmine && echo 'Running tests on https://www.browserstack.com/automate' && ./node_modules/.bin/browserstack-runner",
    "http": "static spec/jasmine -H '{\"Cache-Control\": \"no-cache, must-revalidate\"}'",
    "deploy-s3": "node tasks/deploy.js",
    "precommit": "lint-staged && grunt dist && git add -A docs/index.html && git add -A docs/dist/*"
  },
  "lint-staged": {
    "*.js": [
      "./node_modules/.bin/prettier --write --use-tabs --single-quote --print-width 100",
      "./node_modules/.bin/eslint --config ./.eslintrc.json",
      "git add"
    ]
  },
  "devDependencies": {
    "alce": "^1.0.0",
    "babel-eslint": "^7.1.1",
    "browserstack-runner": "^0.5.0",
    "chalk": "^1.1.3",
    "css-parse": "^2.0.0",
    "eslint": "^3.8.1",
    "eslint-config-prettier": "^1.7.0",
    "eslint-config-tradeshift": "next",
    "eslint-plugin-jasmine": "^2.2.0",
    "eslint-plugin-prettier": "^2.0.1",
    "glob": "^7.1.1",
    "grunt": "^1.0.1",
    "grunt-asciify": "^0.2.2",
    "grunt-concurrent": "^2.3.1",
    "grunt-contrib-clean": "^1.0.0",
    "grunt-contrib-compress": "^1.4.1",
    "grunt-contrib-concat": "^1.0.1",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-contrib-cssmin": "^2.0.0",
    "grunt-contrib-less": "^1.4.0",
    "grunt-contrib-uglify": "^2.2.0",
    "grunt-contrib-watch": "^1.0.0",
    "grunt-devserver": "^0.6.0",
    "grunt-exec": "^2.0.0",
    "grunt-release-ts": "^0.14.3",
    "grunt-spiritual-build": "^0.1.18",
    "grunt-spiritual-dox": "^0.1.1",
    "grunt-spiritual-edbml": "^0.3.0",
    "husky": "^0.13.3",
    "lint-staged": "^3.4.0",
    "load-grunt-tasks": "^3.5.2",
    "node-static": "^0.7.9",
    "prettier": "^1.2.2",
    "s3": "^4.4.0"
  },
  "keywords": [
    "tradeshift",
    "ts",
    "ui",
    "components",
    "library",
    "reusable",
    "webcomponents",
    "client-side",
    "javascript",
    "css",
    "spiritual",
    "edbml",
    "tsml",
    "edb"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Tradeshift/tradeshift-ui.git"
  }
}
