{
	"name": "tradeshift-ui",
	"version": "10.0.1-beta.3",
	"private": true,
	"description": "The Tradeshift UI Library & Framework",
	"homepage": "https://ui.tradeshift.com/",
	"bugs": {
		"url": "https://github.com/Tradeshift/tradeshift-ui/issues",
		"email": "dsp@tradeshift.com"
	},
	"author": "Tradeshift UI Team",
	"contributors": [
		"Wired Earp <jmo@tradeshift.com> (https://github.com/wiredearp)",
		"Leo Zhang <lza@tradeshift.com> (https://github.com/zdlm)",
		"Daniel Spitzer <dsp@tradeshift.com> (https://github.com/sampi)"
	],
	"license": "SEE LICENSE IN LICENSE.md",
	"engines": {
		"node": ">=8.0.0"
	},
	"scripts": {
		"****** LINT *****": "",
		"prelint": "run-p -n prettier prettier-less prettier-json",
		"lint": "yarn run eslint",
		"eslint": "eslint --cache --config ./.eslintrc.json .",
		"prettier": "prettier --write \"**/*.js\"",
		"prettier-less": "prettier --write ---parser postcss \"**/*.less\"",
		"prettier-json": "prettier --write ---parser json \"**/*.json\"",
		"****** DIST *****": "",
		"build": "grunt dist",
		"deploy-s3": "node tasks/deploy.js",
		"deploy-ali-oss": "node tasks/deploy_ali_oss.js",
		"release": "release-it --config=.release.json",
		"prelease": "release-it --config=.prerelease.json",
		"****** TEST *****": "",
		"test": "yarn run eslint & grunt jasmine & wait && node tasks/browserstack.js",
		"http": "static spec/jasmine -H '{\"Cache-Control\": \"no-cache, must-revalidate\"}'",
		"****** NODE *****": "",
		"postinstall": "cd docs && yarn",
		"precommit": "lint-staged && grunt exec:docs_dist && git add -A docs"
	},
	"lint-staged": {
		"*.js": ["prettier --write", "eslint --config ./.eslintrc.json", "git add"],
		"*.json": ["prettier --write --parser json", "git add"],
		"*.less": ["prettier --write --parser postcss", "git add"]
	},
	"browserslist": ["Last 2 versions", "ie 11"],
	"devDependencies": {
		"alce": "^1.2.0",
		"ali-oss": "^4.10.1",
		"autoprefixer": "^7.1.4",
		"babel-eslint": "^7.2.3",
		"browserstack-runner": "^0.5.2",
		"chalk": "^1.1.3",
		"eslint": "3.19.0",
		"eslint-config-prettier": "2.0.0",
		"eslint-config-standard": "10.2.1",
		"eslint-config-tradeshift": "4.2.1",
		"eslint-plugin-import": "2.2.0",
		"eslint-plugin-jasmine": "2.6.0",
		"eslint-plugin-node": "5.0.0",
		"eslint-plugin-promise": "3.5.0",
		"eslint-plugin-standard": "3.0.1",
		"glob": "^7.1.2",
		"grunt": "^1.0.1",
		"grunt-asciify": "^0.2.2",
		"grunt-concurrent": "^2.3.1",
		"grunt-contrib-clean": "^1.0.0",
		"grunt-contrib-compress": "^1.4.3",
		"grunt-contrib-concat": "^1.0.1",
		"grunt-contrib-copy": "^1.0.0",
		"grunt-contrib-uglify": "^3.0.1",
		"grunt-contrib-watch": "^1.0.0",
		"grunt-devserver": "^0.6.0",
		"grunt-exec": "^2.0.0",
		"grunt-postcss": "^0.8.0",
		"grunt-size-report": "^0.1.4",
		"grunt-spiritual-build": "^0.1.18",
		"grunt-spiritual-dox": "^0.1.2",
		"grunt-spiritual-edbml": "^0.3.0",
		"husky": "^0.14.3",
		"less": "^2.7.2",
		"lint-staged": "^4.2.1",
		"load-grunt-tasks": "^3.5.2",
		"node-static": "^0.7.10",
		"npm-run-all": "^4.1.1",
		"postcss-clean": "^1.0.2",
		"postcss-less-engine": "^0.6.2",
		"prettier": "1.7.0",
		"release-it": "2.8.5",
		"s3": "^4.4.0"
	},
	"keywords": [
		"tradeshift",
		"ts",
		"ui",
		"components",
		"library",
		"reusable",
		"webcomponents",
		"client-side",
		"javascript",
		"css",
		"spiritual",
		"edbml",
		"tsml",
		"edb"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Tradeshift/tradeshift-ui.git"
	}
}
